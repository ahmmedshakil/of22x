FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
    movingWall
    {
        type            wall;
        nFaces          1;
        startFace       0;
    }
    fixedWalls
    {
        type            wall;
        nFaces          3;
        startFace       1;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          2;
        startFace       4;
    }
)

